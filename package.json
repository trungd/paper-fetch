{
  "name": "paper-fetch",
  "version": "1.0.4",
  "description": "Search papers and fetch metadata from open access APIs and services",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "paper": "dist/cli.js"
  },
  "author": "Trung Dang",
  "license": "MIT",
  "devDependencies": {
    "@types/inquirer": "^9.0.3",
    "@types/jest": "^29.2.4",
    "@types/lodash": "^4.14.191",
    "fast-xml-parser": "^4.0.12",
    "jest": "^29.3.1",
    "lodash": "^4.17.21",
    "node-fetch": "^2.6.6",
    "ts-jest": "^29.0.3",
    "typescript": "^4.9.4"
  },
  "scripts": {
    "prepublish": "yarn run build",
    "build": "tsc",
    "test": "jest"
  },
  "jest": {
    "preset": "ts-jest",
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg|@rneui|@sentry|@invertase)"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/e2e/",
      "<rootDir>/node_modules/",
      "__mocks__",
      "<rootDir>/dist/"
    ]
  },
  "dependencies": {
    "commander": "^9.4.1",
    "inquirer": "^8.0.0"
  }
}
